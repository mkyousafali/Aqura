# ===========================================
# ENVIRONMENT VARIABLES EXAMPLE
# ===========================================
# Copy this file to .env and fill in your actual values

# ===========================================
# SUPABASE CONFIGURATION
# ===========================================
PUBLIC_SUPABASE_URL=https://your-project.supabase.co
PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ===========================================
# APPLICATION CONFIGURATION
# ===========================================
VITE_APP_NAME=Aqura
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Your AI management system

# ===========================================
# ENVIRONMENT CONFIGURATION
# ===========================================
NODE_ENV=development
VITE_DEV_MODE=true

# ===========================================
# PWA CONFIGURATION
# ===========================================
VITE_PWA_NAME=Aqura Management System
VITE_PWA_SHORT_NAME=Aqura
VITE_PWA_DESCRIPTION=PWA-first windowed management platform
VITE_PWA_THEME_COLOR=#1e40af
VITE_PWA_BACKGROUND_COLOR=#ffffff

# ===========================================
# DATABASE CONFIGURATION
# ===========================================
DATABASE_URL=postgresql://username:password@host:port/database

# ===========================================
# SECURITY & ANALYTICS
# ===========================================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_SECURE_MODE=true

# ===========================================
# AI CONFIGURATION
# ===========================================
VITE_OPENAI_API_KEY=sk-your-openai-api-key-here

# ===========================================
# DEVELOPMENT TOOLS
# ===========================================
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info