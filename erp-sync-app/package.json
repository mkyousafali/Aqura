{
  "name": "aqura-tunnel",
  "version": "1.0.0",
  "description": "Aqura-tunnel - SQL Server to Supabase Bridge",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.aqura.tunnel",
    "productName": "Aqura-tunnel",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "icon.ico"
    }
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "better-sqlite3": "^12.4.6",
    "mssql": "^12.1.1"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9"
  }
}
